  "keywords": ["simplepie", "symfony", "bundle"],
  "license": "GPL-3.0-only",
  "version": "0.1.0",
  "authors": [
    {"name": "Volca Gnomes", "email": "volcagnomes@gmail.com"}
  ],
  "dependencies": {
    
  },
